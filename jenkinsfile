pipeline{
  agent any
  
  stages{
    stage('Pipeline start'){
      steps{
        echo 'Initiating a jenkins pipeline' 
        }
      }
    }
  
    stage{
      stage('Build'){
        steps{
          echo 'Building' 
          }
        }
      }
  
    stage{
      stage('Deploy'){
        steps{
          echo 'Deploying' 
          }
        }
    }
}
